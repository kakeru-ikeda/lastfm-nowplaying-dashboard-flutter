{
  "name": "lastfm-nowplaying-dashboard-server",
  "version": "1.0.0",
  "description": "Node.js server for serving Flutter web build",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:https": "node server.js --https",
    "dev": "nodemon server.js",
    "dev:https": "nodemon server.js --https",
    "clean": "flutter clean",
    "deps": "flutter pub get",
    "generate": "dart run build_runner build --delete-conflicting-outputs",
    "build": "npm run clean && npm run deps && npm run generate && flutter build web --web-renderer html --no-tree-shake-icons",
    "build:serve": "npm run build && npm start",
    "build:serve:https": "npm run build && npm run start:https"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "flutter",
    "web",
    "express",
    "static-server"
  ],
  "author": "",
  "license": "MIT"
}
